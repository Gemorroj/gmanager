{
    "name": "gemorroj/gmanager",
    "description": "php file manager and more",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "autoload": {
        "classmap": [
            "lib/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "ext-zip": "*",
        "pear/archive_tar": "^1.4",
        "symfony/polyfill-mbstring": "^1.8",
        "symfony/process": "^4.4||^5.0||^6.0",
        "gemorroj/syntax": "^4.0"
    },
    "suggest": {
        "ext-rar": "Support RAR archives"
    }
}
